{
    "name": "t3-ui",
    "version": "0.1.22",
    "private": false,
    "author": "wangzhenyu",
    "description": "T3基于vue的UI库",
    "main": "./dist/t3-ui.common.js",
    "files": [
        "dist/*",
        "src/*",
        "public/*",
        "*.json",
        "*.js"
    ],
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build-bundle": "vue-cli-service build --target lib --name t3-ui ./src/components/index.js",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "core-js": "^2.6.5",
        "eslint-plugin-promise": "^4.1.1",
        "eslint-plugin-standard": "^4.0.0",
        "iview": "^3.4.0",
        "vue": "^2.6.10",
        "vue-router": "^3.0.6"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.6.0",
        "@vue/cli-plugin-eslint": "^3.6.0",
        "@vue/cli-service": "^3.6.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-html": "^5.0.3",
        "eslint-plugin-vue": "^5.0.0",
        "html-webpack-plugin": "^3.2.0",
        "ts-loader": "^5.4.4",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5",
        "vue-template-compiler": "^2.5.21"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}